{
  "name": "potato-a11y",
  "version": "0.0.1",
  "description": "Accessible component library website",
  "main": "index.js",
  "scripts": {
    "build": "gulp build",
    "cypress:open": "cypress open",
    "inject": "node --experimental-modules ./scripts/inject-code.mjs",
    "lint": "npm run lint:scss && npm run lint:js",
    "lint:js": "eslint \"src/js/**/*.js\"",
    "lint:scss": "stylelint \"src/sass/**/*.scss\"",
    "start": "gulp",
    "start:test": "npx http-server ./dist --port 3030 -c-1",
    "test": "cypress run --config video=false"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:potato/own-things/potato-material.git"
  },
  "author": "Ahmed Abuelgasim",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "browser-sync": "^2.26.7",
    "cypress": "4.0.1",
    "eslint": "6.6.0",
    "eslint-plugin-cypress": "2.7.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-terser": "^1.2.0",
    "markdown-it": "^10.0.0",
    "stylelint": "11.1.1",
    "stylelint-config-standard": "19.0.0",
    "stylelint-order": "3.1.1",
    "stylelint-scss": "3.13.0"
  },
  "browserslist": [
    "last 2 versions",
    "not IE > 9"
  ],
  "eslintIgnore": [
    "**/*.min.js"
  ],
  "customProperties": {
    "componentLibrary": "asce"
  }
}
